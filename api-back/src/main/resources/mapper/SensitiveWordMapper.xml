<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daken.project.mapper.SensitiveWordMapper">

    <resultMap id="BaseResultMap" type="com.daken.common.entity.SensitiveWord">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="word" column="word" jdbcType="VARCHAR"/>
            <result property="word_type" column="word_type" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,word,word_type
    </sql>
</mapper>
